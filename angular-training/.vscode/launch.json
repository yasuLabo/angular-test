{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Angular: Edge でデバッグ",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "trace": false,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/src/*"
      }
    }
  ]
}
